<h3>Subject Names</h3>
<%
  typeArray = [["ConferenceName", "ConferenceName"],["CorporateName", "CorporateName"],["FamilyName", "FamilyName"],["Name", "Name"], ["PersonalName", "PersonalName"]]
%>

<% 
  i = 0
  @creators.each do |creat|
	tmp = JSON.parse(creat.to_json)
%>
	<%= render :partial => 'shared/edit_fields/creator_edit_fields' , :locals => {:f => f, :object_type => object_type, :typeArray => typeArray, :selected_type => "#{tmp['name']}", :index => "#{i}", :selected_label => "#{tmp['label']}", :selected_value => "#{tmp['value']}"}%>       
<% 
	i = i+1
  end 
%>

<p class="addIcon"><%= link_to_add_dynamic_fields "+", f, "#{object_type}", typeArray, "creator", "#{i}", 1%></p>
